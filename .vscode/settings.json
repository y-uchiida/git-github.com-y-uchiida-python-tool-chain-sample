{
    "mypy-type-checker.args": [
      "--follow-imports=silent",
      "--ignore-missing-imports",
      "--show-column-numbers",
      "--no-pretty",
      "--check-untyped-defs"
    ],
    "[python]": {
      "editor.defaultFormatter": "ms-python.black-formatter",
      "editor.formatOnSave": true
    },
    "editor.defaultFoldingRangeProvider": "ms-python.black-formatter"
}